{
  "extends": [
    "plugin:vue/vue3-recommended"
  ],
  "parserOptions": {
    "ecmaVersion":2020,
    "sourceType": "module",
    "vueFeatures": {
      "filter": true,
      "interpolationAsNonHTML": false
    }
  },
  "parser": "vue-eslint-parser",
  "env": {
    "browser": true,
    "node": true
  },
  "rules": {
      "semi": ["error", "always"],
      "quotes": ["error", "double"]
  }
}
